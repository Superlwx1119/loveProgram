/* pages/login/login.wxss */
.content{
  width: 750rpx;
  height: 100%;
  position: relative;
}
.background{
  width: 100%;
  height: 100%;
}
.formData{
  position: absolute;
  top: 60%;
  left: 50%;
  transform: translateX(-50%);
  width: 500rpx;
  border: 1rpx solid black;
  border-radius: 10rpx;
}
.center{
  width: 500rpx;
  display: flex;
  justify-content: center;
  margin: 30rpx 0;
}
.center text{
  font-size: 30rpx;
}
.center input{
  width: 300rpx;
  margin-left: 20rpx;
  border-bottom: 1rpx solid black;
}
.center button{
  /* height: 100rpx; */
}